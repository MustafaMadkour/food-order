         <script type="text/javascript">
            encodedPayuId='23b515dfdf0161ee2396ba143e118c2a';
            vcoMerchantkey='';
            externalClientId='';
            var isMcpEnabled = "";
            var paypalMcpProductType = "";
            var citiRewardsCatalogue = "";
            var dccHardRestrict = ""; 
            var surchargeData = ''; 
            var transaction_fee = "226.00"; 
            var vijaya_white_label_merchant = ""; 
            function toogleFeedback(callback){
                if(!$("#FeedbackPopup").is(":visible")){
                    $("#FeedbackPopup").show().animate({right:0}, 1000);
                }else{
                    $("#FeedbackPopup").animate({right:'-275px'}, 1000, function(){
                        $("#FeedbackPopup").hide();
                        if(typeof callback == 'function') callback();
                    });
                }
            }
            
            function submitFeedback(txnId , feedback_url){
            
                var error = false;
                if(!$("#FeedbackContent").val() || (/^\s*$/.test($("#FeedbackContent").val()))) {
                    error = true;
                    $("#FeedbackContent").addClass("Feedback_error");
                }
            
                
                if(!error) {
                    jQuery.ajax({
                        type : "POST",
                        url : feedback_url,
                        data : $("#feedbackForm").serialize(),
                        success: function() {
                            $("#FeedbackPopup #heading_para").hide();
                            $("#FeedbackPopup .feedback_body").hide();
                            $("#FeedbackPopup #FeedbackThanks").fadeIn('fast');
                            $("#FeedbackPopup #FeedbackSubmit").hide();
                            $("#FeedbackPopup #ReturnToPaymentPage").fadeIn('fast');
                            $("#FeedbackPopup #captcha").hide();
                            setTimeout(function(){
                                if($("#FeedbackPopup").is(":visible")) {
                                    toogleFeedback(function(){
                                        /*$("#FeedbackPopup  #FeedbackLoading").hide();
                                        $("#FeedbackPopup .feedback_body").fadeIn('fast');
                                        $("#FeedbackPopup #FeedbackThanks").hide();
                                        $("#FeedbackPopup #FeedbackSubmit").fadeIn('fast');
                                        $("#FeedbackPopup #ReturnToPaymentPage").hide();
                                        $("#FeedbackPopup #heading_para").show();*/
                                    });
                                }
                            }, 30000);
                        },
                        error: function() {
                            $("#captcha input[type='text']").addClass("Feedback_error");
                        }
            
                    });
                    /*$("#FeedbackContent").val("");
                    $('#CharLeft').empty().append('(512 characters left)');*/
                }
            }
            
            function countRemaining(){
                var txtAreaObj = $('#FeedbackContent');
                var cntObj = $('#CharLeft');
                var max_length = 512;
                if(txtAreaObj.val().length >= max_length){
                    txtAreaObj.val(txtAreaObj.val().substring(0, max_length));
                }
                if(cntObj){
                    var rem = parseInt(max_length, 10) - parseInt(txtAreaObj.val().length, 10);
                    rem     = isNaN(rem) ? 0 : rem;
                    var m = rem<2?" character":" characters";
                    cntObj.html("(" + rem + m +  " left)");
                }
            }
            
            function removeError_feedback(event){
                var $target = $(event.currentTarget);
                var keycode = getKeyCode(event);
                if($target.hasClass("Feedback_error")  && keycode !== 8) {
                    $target.removeClass("Feedback_error");
                }
            }
            
            function closeFeedback() {
                toogleFeedback(function(){
                });
             }
            
            window.document.onkeydown = function (e)
              {
                if (!e) e = event;
                if (e.keyCode == 27 && $("#FeedbackPopup").is(":visible"))
                  toogleFeedback();
              }
         </script>
		
