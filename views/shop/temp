      <script>
         value_sele1 = 'null';
         value_sele2='null';
         $('#netbanking_select').keydown(function(){
          setTimeout(function(){
          var clear_val=$('#netbanking_select').val();
            if(clear_val!=value_sele1){
              sele = true;
            }
            if(clear_val==value_sele1){
              sele = false;
            }
            $('#netbanking_select').valid();
         
          if(sele){
            clear_all_sel();
          }
            var rev_sel=$('#netbanking_select').val();
                var selection1 = $('#b-icon-1').attr('class').split('icon-bank icon-')[1];
              if(rev_sel == selection1){
                clear_all_sel();
                $('#bank-rect-id-1').attr("style","border:2px solid #00adf2;");
                $('#radio-1').attr("checked",true);
              }
                  var selection2 = $('#b-icon-2').attr('class').split('icon-bank icon-')[1];
              if(rev_sel == selection2){
                clear_all_sel();
                $('#bank-rect-id-2').attr("style","border:2px solid #00adf2;");
                $('#radio-2').attr("checked",true);
              }
                  var selection3 = $('#b-icon-3').attr('class').split('icon-bank icon-')[1];
              if(rev_sel == selection3){
                clear_all_sel();
                $('#bank-rect-id-3').attr("style","border:2px solid #00adf2;");
                $('#radio-3').attr("checked",true);
              }
                  var selection4 = $('#b-icon-4').attr('class').split('icon-bank icon-')[1];
              if(rev_sel == selection4){
                clear_all_sel();
                $('#bank-rect-id-4').attr("style","border:2px solid #00adf2;");
                $('#radio-4').attr("checked",true);
              }
                  var selection5 = $('#b-icon-5').attr('class').split('icon-bank icon-')[1];
              if(rev_sel == selection5){
                clear_all_sel();
                $('#bank-rect-id-5').attr("style","border:2px solid #00adf2;");
                $('#radio-5').attr("checked",true);
              }
                  var selection6 = $('#b-icon-6').attr('class').split('icon-bank icon-')[1];
              if(rev_sel == selection6){
                clear_all_sel();
                $('#bank-rect-id-6').attr("style","border:2px solid #00adf2;");
                $('#radio-6').attr("checked",true);
              }
                $('#netbanking_select').trigger('change');
         },0);
         
         });
         
         $('#cash_card_select').keydown(function(){
           setTimeout(function(){
         
              var clear_val=$('#cash_card_select').val();
              if(clear_val!=value_sele2){
                sele = true;
              }
              if(clear_val==value_sele2){
         
                sele = false;
              }
              $('#cash_card_select').valid();
            if(sele){
              clear_all_sel();
            }
         
              var rev_sel=$('#cash_card_select').val();
                $('#cash_card_select').trigger('change');
         
            },0);
         });
         
         function bankicons(bankRect,radio,bankIcon){
          clear_all_sel();
          if(bankRect!=null){
           $('#'+bankRect).attr("style","border:2px solid #00adf2;");
           var selection = $('#'+bankIcon).attr('class').split('icon-bank icon-')[1];
          $('#netbanking_select').val(selection.toString());
          value_sele1 = $('#netbanking_select').val();
            $('#netbanking_select').valid();
         
            $('#netbanking_select').trigger('change');
         }
         }
         function walleticons(bankRect,radio,bankIcon){
          clear_all_sel();
          if(bankRect!=null){
           $('#'+bankRect).attr("style","border:2px solid #00adf2;");
           var selection = $('#'+bankIcon).attr('class').split('icon-bank icon-')[1];
          $('#cash_card_select').val(selection.toString());
            value_sele2 = $('#cash_card_select').val();
            $('#cash_card_select').valid();
         
            $('#cash_card_select').trigger('change');
         }
         }
         
         function clearselection(nbw){
          if(nbw == 'bank'){
            var clear_val=$('#netbanking_select').val();
            if(clear_val!=value_sele1){
              sele = true;
            }
            if(clear_val==value_sele1){
         
              sele = false;
            }
            $('#netbanking_select').valid();
         
          }
          if(nbw == 'wallet'){
            var clear_val=$('#cash_card_select').val();
            if(clear_val!=value_sele2){
              sele = true;
            }
            if(clear_val==value_sele2){
              sele = false;
            }
            $('#cash_card_select').valid();
         
          }
          if(sele){
          clear_all_sel();
          }
         
          if(nbw=='bank'){
           var rev_sel=$('#netbanking_select').val();
             var selection1 = $('#b-icon-1').attr('class').split('icon-bank icon-')[1];
           if(rev_sel == selection1){
             clear_all_sel();
             $('#bank-rect-id-1').attr("style","border:2px solid #00adf2;");
             $('#radio-1').attr("checked",true);
           }
             var selection2 = $('#b-icon-2').attr('class').split('icon-bank icon-')[1];
           if(rev_sel == selection2){
             clear_all_sel();
             $('#bank-rect-id-2').attr("style","border:2px solid #00adf2;");
             $('#radio-2').attr("checked",true);
           }
             var selection3 = $('#b-icon-3').attr('class').split('icon-bank icon-')[1];
           if(rev_sel == selection3){
             clear_all_sel();
             $('#bank-rect-id-3').attr("style","border:2px solid #00adf2;");
             $('#radio-3').attr("checked",true);
           }
             var selection4 = $('#b-icon-4').attr('class').split('icon-bank icon-')[1];
           if(rev_sel == selection4){
             clear_all_sel();
             $('#bank-rect-id-4').attr("style","border:2px solid #00adf2;");
             $('#radio-4').attr("checked",true);
           }
             var selection5 = $('#b-icon-5').attr('class').split('icon-bank icon-')[1];
           if(rev_sel == selection5){
             clear_all_sel();
             $('#bank-rect-id-5').attr("style","border:2px solid #00adf2;");
             $('#radio-5').attr("checked",true);
           }
             var selection6 = $('#b-icon-6').attr('class').split('icon-bank icon-')[1];
           if(rev_sel == selection6){
             clear_all_sel();
             $('#bank-rect-id-6').attr("style","border:2px solid #00adf2;");
             $('#radio-6').attr("checked",true);
           }
           
          }
         
          if(nbw=='wallet'){
           var rev_sel=$('#cash_card_select').val();
            }
         }
         
         function clear_all_sel(){
          $('.bank-rect').attr("style","border:1px solid #c9c9c9;");
          $(".radio-custom").attr("checked",false);
          $(".radio-customw").attr("checked",false);
         
         }
         
